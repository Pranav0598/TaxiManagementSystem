    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_publicLayout.cshtml";
}



<div class="container">
    <div class="row">
        <div class="col-sm-8">
            <h3> Successfull Registrarion</h3>
            <p>You have been sucessfully registered. Please proceed to  @Html.ActionLink("Login", "Login", "Users", new { @class = "btn btn-primary" })</p>
