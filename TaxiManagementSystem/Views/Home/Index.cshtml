@model TaxiManagementSystem.Models.Users


@{
    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="container">
    <div class="row">
        <p>
            Welcome to the Taxi management system.
        </p>
    </div>
    <div class="row">
        <h5>Driver details</h5>
    </div>
        <div class="row">
            <div class="col-sm-3 text-right"><b>User Id :</b></div>
            <div class="col-sm-9">@Model.UserId</div>
        </div>
        <div class="row">
            <div class="col-sm-3 text-right"><b>Username :</b></div>
            <div class="col-sm-9">@Model.UserName</div>
        </div>
        <div class="row">
            <div class="col-sm-3 text-right"><b>Name :</b></div>
            <div class="col-sm-9">@(Model.FirstName +" "+ Model.LastName)</div>
        </div>
        <div class="row">
            <div class="col-sm-3 text-right"><b>Age :</b></div>
            <div class="col-sm-9">@Model.Age</div>
        </div>
        <div class="row">
            <div class="col-sm-3 text-right"><b>Email :</b></div>
            <div class="col-sm-9">@Model.Email</div>

        </div>
    </div>

